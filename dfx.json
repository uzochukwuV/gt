{
  "version": 1,
  "dfx": "0.18.0",
  "canisters": {
    "backend": {
      "type": "rust",
      "package": "backend",
      "candid": "src/backend/backend.did"
    },
    "marketplace": {
      "type": "rust",
      "package": "marketplace",
      "candid": "src/marketplace/marketplace.did"
    },
    "lending": {
      "type": "rust",
      "package": "lending",
      "candid": "src/lending/lending.did"
    },
    "ai_verifier": {
      "type": "rust",
      "package": "ai_verifier",
      "candid": "src/ai_verifier/ai_verifier_simple.did"
    },
    "frontend": {
      "dependencies": ["backend", "marketplace", "lending", "ai_verifier"],
      "type": "assets",
      "source": ["src/frontend/dist"]
    }
  },
  "defaults": {
    "build": {
      "packtool": "npm --prefix src/frontend run build",
      "args": ""
    }
  },
  "output_env_file": ".env"
}
